{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "AdGuardHomeHA": "Debug",
      "System.Net.Http.HttpClient": "Warning"
    }
  },
  "AdGuardHomeHA": {
    "AdGuardHome": {
      "BaseUrl": "http://your-adguard-home:3000",
      "Username": "admin",
      "Password": ""
    },
    "Machines": [
      {
        "Name": "Primary Server",
        "IpAddress": "192.168.1.100",
        "Priority": 1,
        "TimeoutMs": 5000
      },
      {
        "Name": "Secondary Server",
        "IpAddress": "192.168.1.101",
        "Priority": 2,
        "TimeoutMs": 5000
      }
    ],
    "DnsRewrites": [
      "service.example.com",
      "api.example.com",
      "app.example.com"
    ],
    "Monitoring": {
      "CheckIntervalSeconds": 30,
      "RetryAttempts": 3,
      "RetryDelayMs": 1000
    }
  }
}
